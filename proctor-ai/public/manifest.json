{
    "manifest_version": 3,
    "name": "Proctor AI",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "tabs"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]
}